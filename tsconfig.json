{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es2017"
    ],
    "types": [
      "./lib/types"
    ],
    "paths": {
      "modlib": [
        "./lib/modlib"
      ]
    },
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
